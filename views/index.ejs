                                    <div class="col">
                                        <h5 class="card-title text-uppercase text-muted mb-0">Total Beta Users</h5>
                                        <span class="h2 font-weight-bold mb-0"><%= data.betas %></span>
                                    </div>
                                    <div class="col-auto">
                                        <div class="icon icon-shape bg-gradient-red text-white rounded-circle shadow">
                                            <i class="fas fa-flask"></i>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-3 col-md-6">
                        <div class="card card-stats">
